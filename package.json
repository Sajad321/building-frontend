{
  "name": "building-managment-system",
  "version": "0.6.0",
  "description": "System for managing a building",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/sajad321/building-frontend"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/sajad321/building-frontend"
  },
  "author": {
    "name": "BeSmarty",
    "email": "admin@besmarty.com",
    "url": "https://github.com/sajad321"
  },
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "open",
    "open-source",
    "postcss",
    "react",
    "reactjs",
    "source",
    "webpack"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack serve --hot --host localhost --config=./webpack.dev.config.js --mode development",
    "build:python": "PyInstaller --clean -F --onefile -y .\\main.spec",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "prepackage": "rimraf ./build && mkdir build && rimraf ./dist && mkdir dist && rimraf ./release-builds && mkdir release-builds",
    "package": "npm run build && electron-packager --win32metadata.requested-execution-level=requireAdministrator ./ --out=./build --prune --asar --extra-resource=py_dist --ignore=release-builds --ignore=docs --ignore=for-code",
    "postpackage": "electron-builder --prepackaged ./build/building-managment-system-win32-x64 --publish=never"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "electron-context-menu": "^3.1.1",
    "jquery": "^3.6.0",
    "node-sass": "^6.0.1",
    "popper.js": "^1.16.1",
    "print-js": "^1.6.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-paginate": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.4",
    "react-toastify": "^4.5.2",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^7.0.2",
    "cross-env": "^7.0.3",
    "electron": "^13.1.7",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.2.0",
    "electron-wix-msi": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.2.0",
    "webpack": "^5.24.4",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "build": {
    "extends": null,
    "productName": "BMS",
    "appId": "com.app.bms",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "release-builds"
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  }
}
